<launch>

  <!-- Load URDF in param server -->
  <include file="$(find reem_description)/robots/upload_reem.launch" />

  <!-- Robot state publisher -->
  <node pkg="robot_state_publisher" type="state_publisher" name="robot_state_publisher">
    <param name="publish_frequency" type="double" value="25.0" />
    <param name="tf_prefix" type="string" value="" />
  </node>

  <!-- Default controllers -->
  <include file="$(find reem_controller_configuration)/launch/reem_default_controllers.launch" />

  <!-- Point head action TODO: Move to default controllers! -->
  <include file="$(find reem_bringup)/launch/point_head_action.launch" />

</launch>